<Application x:Class="MinimalistMusicPlayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="res/Colors.xaml" />
				<ResourceDictionary Source="res/Slider.xaml" />
				<ResourceDictionary Source="res/Scrollbar.xaml" />
				<ResourceDictionary Source="res/AlphaButton.xaml" />
				<ResourceDictionary Source="res/AlphaButtonToggle.xaml" />
				<ResourceDictionary Source="res/BackgroundButton.xaml" />
				<ResourceDictionary Source="res/BackgroundButton2.xaml" />
				<ResourceDictionary Source="res/BackgroundButtonToggle.xaml" />
				<ResourceDictionary Source="res/PlaylistItem.xaml" />
				<ResourceDictionary Source="res/ToolTip.xaml" />
				<ResourceDictionary Source="res/Vectors.xaml" />
			</ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>